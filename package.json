{
  "name": "reservation-app-fe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.14.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.3.3",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "querystring-es3": "^0.2.1",
    "react": "^18.3.0",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.27.0",
    "superagent": "^10.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "generate-api-client": "npx @openapitools/openapi-generator-cli generate -i http://localhost:8080/v3/api-docs -g javascript -o ./src/generated-api-client",
    "start": "npm run generate-api-client && cross-env NODE_ENV=production node server.js",
    "dev": "npm run generate-api-client && cross-env NODE_ENV=development react-app-rewired start",
    "build": "npm run generate-api-client && react-app-rewired build",
    "build-legacy": "npm run generate-api-client && cross-env NODE_OPTIONS=--openssl-legacy-provider react-app-rewired build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "react-app-rewired": "^2.2.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
